<Window x:Class="Launcher.SvgViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        mc:Ignorable="d"
        Title="SvgViewer" Height="450" Width="800">

<DockPanel>

<Slider Minimum="0.1" Maximum="10" DockPanel.Dock="Top" Value="{Binding ZoomFactor}"></Slider>

    <ScrollViewer PanningMode="Both" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" DockPanel.Dock="Bottom">
        <svgc:SvgViewbox x:Name="_viewBox" >
       
        </svgc:SvgViewbox>
    </ScrollViewer>
</DockPanel>

    

</Window>
